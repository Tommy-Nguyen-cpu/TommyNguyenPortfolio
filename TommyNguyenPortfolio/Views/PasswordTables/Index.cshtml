@model IEnumerable<TommyNguyenPortfolio.Models.PasswordTable>

    
@{
    ViewData["Title"] = "Index";
}

<h1>Users Information</h1>

<p>
    <a asp-action="Create" class="btn btn-success">Create New</a>
</p>
<table class="table" style="background-color:white">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PasswordTableId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Username)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClientID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PasswordTableId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Username)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClientID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PasswordTableId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PasswordTableId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PasswordTableId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
